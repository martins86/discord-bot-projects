{
  "name": "discord-bot-projects",
  "version": "0.0.1",
  "description": "Bot no Discord para suporte a projetos",
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "pre-commit": "npx --no-install lint-staged",
    "prettier-write": "npx prettier --write --ignore-unknown .",
    "prettier-check": "npx prettier --check .",
    "release": "standard-version",
    "start": "node index.js",
    "postinstall": "npx husky install && chmod ug+x .husky/*"
  },
  "license": "MIT",
  "keywords": [
    "bot",
    "discord"
  ],
  "author": "Thiago Martins de Freitas",
  "email": "martins86@gmail.com",
  "url": "https://www.linkedin.com/in/tmfreitas/",
  "homepage": "https://martins86.github.io/discord-bot-projects/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martins86/discord-bot-projects.git"
  },
  "bugs": {
    "url": "https://github.com/martins86/discord-bot-projects/issues",
    "email": "martins86@gmail.com"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "2.8.8",
    "standard-version": "^9.5.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
